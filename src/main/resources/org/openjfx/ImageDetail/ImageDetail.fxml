<VBox minHeight="300.0" minWidth="400.0" styleClass="root, center-align" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.ImageDetailController">
    <children>
        <HBox prefHeight="100.0" prefWidth="378.0">
            <children>
                <HBox HBox.hgrow="NEVER" fx:id="imageViewContainer">
                <ImageView fitHeight="100.0" fitWidth="100.0">
                    <image>
                        <Image url="@../images/placeholder.png" />
                    </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
                </ImageView>
                </HBox>
                <Label
                        styleClass="gap-margin" text=" "></Label>
                <GridPane  styleClass="font-small" HBox.hgrow="ALWAYS" prefHeight="100.0" prefWidth="412.0">
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="384.0" minWidth="10.0" prefWidth="185.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="5.0" prefWidth="210.0" />
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                   <children>
                      <Label fx:id="longitude" text="Longitude" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                      <Label fx:id="latitude" text="Latitude" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                      <Label fx:id="width" text="Width" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                      <Label fx:id="height" text="Height" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                      <Label fx:id="cameraMakeAndModel" text="Camera Make and Model" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Camera" />
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Longitude" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Latitude" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Width" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
                      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Height" GridPane.rowIndex="4" GridPane.columnIndex="0"/>
                   </children>
                </GridPane>
               <!--<columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="384.0" minWidth="10.0" prefWidth="185.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="5.0" prefWidth="210.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>-->

            </children>
        </HBox>
        <Label
                styleClass="gap-margin" text=" "></Label>
        <Button onAction="#switchToList" styleClass="btn, btn-primary" text="Back" />
    </children>

    <stylesheets>
        <URL value="@../styles.css" />
        <URL value="@ImageDetail.css" />
    </stylesheets>
</VBox>
